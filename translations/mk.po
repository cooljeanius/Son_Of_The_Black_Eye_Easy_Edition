#: Son_Of_The_Black_Eye_Easy_Edition/scenarios/03_Toward_Mountains_of_Haag.cfg:617
msgid "Grüü"
msgstr ""

#. [side]: type=General, id=Telthys
#: Son_Of_The_Black_Eye_Easy_Edition/scenarios/14_Back_Home.cfg:122
msgid "Telthys"
msgstr ""

